<Window x:Class="Hospital_Management_System.UploadFile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hospital_Management_System"
        mc:Ignorable="d"
        Title="UploadFile" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnUpload" Content="Upload File" HorizontalAlignment="Left" Margin="585,83,0,0" VerticalAlignment="Top" Height="47" Width="176" Click="btnUpload_Click"/>
        <TextBox x:Name="tbox" HorizontalAlignment="Left" Margin="613,36,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <DataGrid x:Name="datagrid" Margin="0,0,484,0" SelectionChanged="datagrid_SelectionChanged" />
        <TextBox x:Name="tboxTC" HorizontalAlignment="Center" Margin="0,36,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" KeyDown="tboxTC_KeyDown" TextChanged="tboxTC_TextChanged"/>

    </Grid>
</Window>
